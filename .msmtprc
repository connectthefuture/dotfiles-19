# Set default values for all following accounts.
defaults
logfile ~/.msmtp.log

# A freemail service
account fiit
host localhost
port 2525
tls on
tls_starttls on
tls_min_dh_prime_bits 7
tls_certcheck off
auth on
from kramar@fiit.stuba.sk
user kramar
password ...

# Set a default account
account default : fiit
