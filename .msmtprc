# Set default values for all following accounts.
defaults
logfile ~/.msmtp.log

# FIIT
account fiit
host dcs.fiit.stuba.sk
port 465
tls on
tls_starttls on
tls_min_dh_prime_bits 7
tls_certcheck off
auth on
from kramar@fiit.stuba.sk
user kramar
# No password is set, this will result in a keyring lookup
# Use
# ldd `which msmtp`
# to check if msmtp was built with gnome-keyring support

# Set a default account
account default : fiit
